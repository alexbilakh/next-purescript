{
  "name": "next-purescript-example",
  "version": "0.1.0",
  "description": "Example app using Next.js with Purescript",
  "keywords": [
    "Next.js",
    "Purescript",
    "React",
    "SSR"
  ],
  "author": "Jonas Buntinx",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "spago build",
    "bundle": "yarn bundle:clean && yarn bundle:spago && yarn bundle:dce && yarn bundle:next",
    "bundle:clean": "rm -Rf corefn build out_functions out_publish",
    "bundle:spago": "spago build --purs-args '--output corefn --codegen corefn'",
    "bundle:dce": "bash scripts/zephyr.sh",
    "bundle:next": "next build",
    "clean": "rm -Rf .cache .next output build corefn out_functions out_publish node_modules .spago *.lock",
    "dev": "yarn dev:clean && yarn dev:build && yarn dev:serve",
    "dev:clean": "rm -Rf build",
    "dev:build": "spago build --purs-args '--output build'",
    "dev:serve": "spago build --watch & next",
    "id": "pscid",
    "pscid:build": "spago build",
    "postbundle": "next-on-netlify",
    "postinstall": "spago install",
    "start": "yarn start:clean && yarn start:spago && yarn start:next && yarn start:serve",
    "start:clean": "rm -Rf build",
    "start:spago": "spago build --purs-args '--output build'",
    "start:next": "next build",
    "start:serve": "next start",
    "test": "spago test"
  },
  "devDependencies": {
    "pscid": "^2.9.3",
    "purescript": "^0.13.8",
    "purescript-psa": "^0.8.0",
    "purty": "^6.3.0",
    "spago": "^0.19.0",
    "string-replace-loader": "^3.0.1",
    "zephyr": "https://github.com/jonasbuntinx/zephyr.git"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "autoprefixer": "^10.2.1",
    "cross-env": "^7.0.3",
    "next": "^10.0.5",
    "next-on-netlify": "^2.8.1",
    "postcss": "^8.2.4",
    "preact": "^10.5.10",
    "preact-render-to-string": "^5.1.12",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-ssr-prepass": "npm:preact-ssr-prepass@1.1.3",
    "tailwindcss": "^2.0.2",
    "xhr2": "^0.2.0"
  }
}
