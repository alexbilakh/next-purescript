{
  "name": "next-purescript-example",
  "version": "0.1.0",
  "description": "Example app using Next.js with Purescript",
  "keywords": [
    "Next.js",
    "Purescript",
    "React",
    "SSR"
  ],
  "author": "Jonas Buntinx",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "yarn build:spago && yarn build:dce && yarn build:next",
    "build:spago": "spago build --purs-args '--codegen corefn'",
    "build:dce": "bash scripts/zephyr.sh './output/Pages.*'",
    "build:next": "next build",
    "clean": "rm -Rf .next .cache dist output",
    "dev": "spago build && (spago build --watch & next)",
    "id": "pscid",
    "pscid:build": "spago build",
    "postbuild": "next-on-netlify",
    "postinstall": "spago install",
    "start": "yarn clean && yarn build && next start",
    "test": "spago test"
  },
  "devDependencies": {
    "pscid": "^2.9.3",
    "purescript": "^0.13.8",
    "purescript-psa": "^0.7.3",
    "purty": "^6.2.0",
    "spago": "^0.16.0",
    "zephyr": "https://github.com/jonasbuntinx/zephyr.git"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.5.3",
    "cross-env": "^7.0.2",
    "next": "^9.5.3",
    "next-on-netlify": "^2.4.0",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-ssr-prepass": "npm:preact-ssr-prepass@1.1.1",
    "tailwindcss": "^1.8.5"
  }
}
